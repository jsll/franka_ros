<?xml version="1.0"?>
<launch>

  <arg name="robot" default="panda" doc="choose your robot. Possible values: [panda, fr3]" />
  <arg name="arm_id" default="$(arg robot)" />
  <node pkg="tf" type="static_transform_publisher" name="worldtopanda0"
    args="0 0 0 0 0 0 world panda_table_link 100" />
  <rosparam command="load"
    file="$(find franka_example_controllers)/config/franka_example_controllers.yaml" subst_value="true" />
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="kth_cartesian_velocity_effort_interface_controller" />
  <node pkg="rviz" type="rviz" output="screen" name="rviz"
    args="-d $(find franka_example_controllers)/launch/robot.rviz" />


</launch>